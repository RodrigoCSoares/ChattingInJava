<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBox" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Cliente.Conexao.ControllerConexao">
   <children>
      <TextField fx:id="txtNick" onKeyTyped="#terminouDeDigitar" promptText="Digite aqui seu nick" />
      <ComboBox fx:id="cbSalas" onAction="#terminouDeDigitar" promptText="Qual nomeSala deseja conectar?" />
      <Button fx:id="btnConectar" mnemonicParsing="false" onAction="#conectar" text="Conectar" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
